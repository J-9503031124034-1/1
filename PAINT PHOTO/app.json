{
  "expo": {
    "name": "mobile photoshop",
    "slug": "mobile photoshop",
    "version": "42",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "memo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "versionCode": 42,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ec97d4fa3d484a5aaa8a60fbfc94fb.pen_memo",
      "permissions": [],
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      "expo-media-library",
      "expo-document-picker",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0b21184d-d355-4af8-86c7-90d1a3498277"
      }
    },
    "owner": "ec97d4fa3d484a5aaa8a60fbfc94fb"
  }
}
